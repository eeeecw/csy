.tab-bar {
  width: 100%;
  position: fixed;
  bottom: 0;
}
.login-btn {
  display: initial;
  background-color: transparent;
  font-size: 16px;
  color: #2d8cf0;
  padding: 0;
}
.login-btn::after {
  border: none;
}
.me-box {
  width: 100%;
  height: 100%;
  background-color: #f3f3f3;
}
.user-info .head {
  background-color: #fff000;
  padding: 14px 18px 14px 18px;
  border-radius: 0 0 10% 10%;
  vertical-align: top;
}
.user-info .head-img {
  width: 100rpx;
  height: 100rpx;
  vertical-align: top;
  border-radius: 5px;
}
.user-info .head-name {
  margin-left: 10px;
}
.my-list {
  margin: 20rpx;
  padding: 20rpx;
  background-color: #fff;
  border-radius: 5px;
}
.my-list-head {
  display: flex;
  justify-content: space-between;
  padding-bottom: 18rpx;
}
.my-list-head-title {
  font-weight: 500;
}
.my-list-head-btn {
  font-size: 14px;
  color: #2d8cf0;
}
.my-list-body {
  font-size: 14px;
}
.my-list-body-item {
  line-height: 22px;
  display: flex;
}
.my-list-body-item-name {
  flex: 1;
}
.my-list-body-item-edit {
  color: #2d8cf0;
  margin-right: 5px;
}
.my-list-body-item-delete {
  color: #ed4014;
}
.my-list-body-item-default {
  color: #009966;
  margin-right: 5px;
}

